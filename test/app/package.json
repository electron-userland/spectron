{
  "private": "true",
  "name": "@goosewobbler/spectron-e2e-app",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "build": "pnpm run clean:dist && webpack && cp ./src/index.html ./dist && electron-builder",
    "clean": "cross-env rm -rf ./node_modules && pnpm run clean:dist",
    "clean:dist": "cross-env rm -rf ./dist && cross-env mkdir -p ./dist"
  },
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2"
  },
  "build": {
    "asar": true,
    "appId": "com.test.demo",
    "copyright": "goosewobbler",
    "productName": "test",
    "files": [
      "./dist/*"
    ],
    "linux": {
      "executableName": "test",
      "category": "Utility",
      "target": [
        "AppImage"
      ]
    }
  }
}
