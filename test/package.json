{
  "private": "true",
  "name": "@goosewobbler/spectron-e2e",
  "version": "1.0.0",
  "main": "./app/dist/main.js",
  "scripts": {
    "build:app": "pnpm run --filter @goosewobbler/spectron-e2e-app build",
    "build:spectron": "pnpm run --filter @goosewobbler/spectron build",
    "build:wdio-electron-service": "pnpm run --filter wdio-electron-service build",
    "clean": "cross-env rm -rf ./node_modules ./all-logs",
    "test": "pnpm exec wdio spectron.conf.js",
    "test:local": "pnpm run build:wdio-electron-service && pnpm run build:spectron && pnpm run build:app && pnpm run test",
    "lint": "eslint --ignore-path ../.eslintignore \"**/*.{j,t}s\""
  },
  "devDependencies": {
    "@goosewobbler/spectron": "*",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/webdriverio": "^3.0.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@wdio/cli": "^7.16.10",
    "@wdio/local-runner": "^7.16.10",
    "@wdio/mocha-framework": "^7.16.6",
    "expect-webdriverio": "^3.1.4",
    "global-jsdom": "^8.4.0",
    "jsdom": "^19.0.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "webdriverio": "^7.16.10",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "engines": {
    "node": ">=14.17.1"
  }
}
