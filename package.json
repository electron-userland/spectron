    "request": "^2.87.0",
    "split": "^1.0.0",
    "webdriverio": "^4.13.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-roughly": "^1.0.0",
    "check-for-leaks": "^1.0.2",
    "electron": "^8.0.0",
    "husky": "^4.2.1",
    "mocha": "^7.0.1",
    "standard": "^14.3.1",
    "temp": "^0.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.5.3",
